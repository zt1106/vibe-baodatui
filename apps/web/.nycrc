{
  "all": true,
  "exclude-after-remap": false,
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "hooks/**/*.ts",
    "hooks/**/*.tsx",
    "lib/**/*.ts",
    "lib/**/*.tsx"
  ],
  "extension": [".ts", ".tsx", ".js", ".jsx"],
  "exclude": [
    "**/*.stories.{ts,tsx}",
    "tests/**",
    "test/**",
    "**/__tests__/**"
  ],
  "reporter": ["text-summary", "html"],
  "report-dir": "./coverage/e2e",
  "temp-dir": "./.nyc_output"
}
